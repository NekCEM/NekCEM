export COMM_BACKEND=comm_seq
FORTRAN_DEFINES="-DNO_C_MAIN $FORTRAN_DEFINES"
C_DEFINES="-Dr8 -DNO_C_MAIN $C_DEFINES"
export F77="pgf77 -Ktrap=fp -Minform=warn \
  -I$BASEDIR/examples/include \
  -I$BASEDIR/src \
  $FORTRAN_DEFINES"
export CC="pgcc  -Ktrap=fp $C_DEFINES"
export P="-r8"
export EXTRALIBS=""
export LAPACK=" -llapack -lblas "
export LD="pgf77"
OPT_FULL="-O3 -Minline"
OPT_DEBUG="-g -O0"
MAKE=make

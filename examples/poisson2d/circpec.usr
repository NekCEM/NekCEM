c-----------------------------------------------------------------------
      subroutine userini(tt, myhx, myhy, myhz, myex, myey, myez)
c-----------------------------------------------------------------------
      include 'SIZE'
      include 'TOTAL'
      include 'EMWAVE'
      include 'NEKUSE'
      include 'POISSON'

      real tt
      real myhx(lx1,ly1,lz1,lelt)
      real myhy(lx1,ly1,lz1,lelt)
      real myhz(lx1,ly1,lz1,lelt)
      real myex(lx1,ly1,lz1,lelt)
      real myey(lx1,ly1,lz1,lelt)
      real myez(lx1,ly1,lz1,lelt)

      npts=nx1*ny1*nz1*nelt

C...  circ mesh with PEC boundary     
      do i = 1,npts
         xx = xm1(i,1,1,1)
         yy = ym1(i,1,1,1)
         rr = xx**2 + yy**2 
         rr = sqrt(rr)
         write(6,*) 'rr:',i,rr
         charge (i)= -4.0
         spotent(i)= (rr**2-1)
         epotent(i)= 0  
      enddo
      if (nid.eq.0) write(6,*) 'poisson: set charge in userini'

      call poissonq (myex,myey,myez,potent,npts)

      if (nid.eq.0) then
      do i=1,npts
         write(6,*) 'pot: ',nid,i,potent(i), spotent(i)
      enddo
      endif

      call sub3(epotent,spotent,potent,npts)
      errmax= glamax(epotent,npts)
      if (nid.eq.0) write(6,*) 'poisson: potent_maxerr=',errmax

      do i=1,npts
         xx = xm1(i,1,1,1)
         yy = ym1(i,1,1,1)
         rr = xx**2+yy**2
         rr = sqrt(rr)
         theta = atan2(yy,xx)
         e1 = myex(i,1,1,1)
         e2 = myey(i,1,1,1)
         e3 = 0.0
         call rectangular_to_cylindrical(ee1,ee2,ee3,e1,e2,e3,theta)
         hh1= 0.0
         hh2= ee1
         hh3= 0.0
         call cylindrical_to_rectangular(c1,c2,c3,hh1,hh2,hh3,theta)
         myhx(i,1,1,1)= c1
         myhy(i,1,1,1)= c2
         myhz(i,1,1,1)= 0.0
      enddo

      return
      end

c-----------------------------------------------------------------------
      subroutine usersol(tt, myshx, myshy, myshz, mysex, mysey, mysez)
c-----------------------------------------------------------------------
      include 'SIZE'
      include 'TOTAL'
      include 'EMWAVE'

      real tt
      real myshx(lx1,ly1,lz1,lelt)
      real myshy(lx1,ly1,lz1,lelt)
      real myshz(lx1,ly1,lz1,lelt)
      real mysex(lx1,ly1,lz1,lelt)
      real mysey(lx1,ly1,lz1,lelt)
      real mysez(lx1,ly1,lz1,lelt)

      npts=nx1*ny1*nz1*nelt

      do i = 1,npts
         xx = xm1(i,1,1,1)
         yy = ym1(i,1,1,1)
         zz = zm1(i,1,1,1)
         rr = xx**2+yy**2
         rr = sqrt(rr)
         ee1=-2*rr
         ee2= 0
         ee3= 0
         theta= atan2(yy,xx)
         mysex(i,1,1,1)= cos(theta)*ee1-sin(theta)*ee2
         mysey(i,1,1,1)= sin(theta)*ee1+cos(theta)*ee2
         mysez(i,1,1,1)= 0
         myshx(i,1,1,1)= cos(theta)*ee1-sin(theta)*ee2
         myshy(i,1,1,1)= sin(theta)*ee1+cos(theta)*ee2
         myshz(i,1,1,1)= 0
      enddo

      return
      end
